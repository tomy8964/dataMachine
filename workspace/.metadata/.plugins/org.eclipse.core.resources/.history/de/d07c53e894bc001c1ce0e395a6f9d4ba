import java.util.Scanner;
public class p6 {

	public static void main(String[] args) {
		int count=0,a=0,b=0,c=0,d=0,f=0;
		// 스캐너 객체 keyboard 생성
		Scanner keyboard = new Scanner(System.in);
		System.out.println("This program");
		
		
		while(true)
		{
			int score = keyboard.nextInt();
			
			if(score<=-1) break;
			else count++;
			
			if(score>89) a++;
			else if(score>79) b++;
			else if(score>69) c++;
			else if(score>59) d++;
			else f++;
		}
		System.out.println("For the "+count+" numbers you entered");
		System.out.println("the largest value = "+larg);
		System.out.println("the smallest value = "+small);
		System.out.println("and the average is = "+avg);
		keyboard.close();
	}
}










